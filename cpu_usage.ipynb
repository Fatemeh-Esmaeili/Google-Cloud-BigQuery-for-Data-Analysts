    "import psutil\n",
    "\n",
    "def check_cpu_usage(percent):\n",
    "\n",
    "    usage = psutil.cpu_percent()\n",
    "\n",
    "    return usage < percent\n",
    "\n",
    "if not check_cpu_usage(75):\n",
    "\n",
    "    print(\"ERROR! CPU is overloaded\")\n",
    "\n",
    "else:\n",
    "\n",
    "\n",
    "    print(\"Everything ok\")"
   ]
